<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mapper.BooksTypeMapper" >
  <resultMap id="BaseResultMap" type="com.library.system.entity.BooksTypeEntity" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="type" property="type" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id,type
  </sql>
  
  <select id="selectAllType" resultType="map">
    select * from  book_type
  </select>
  
  
  <select id="selectById" resultType="String" parameterType="INTEGER">
    select type from  book_type  where id=#{id}
  </select>
</mapper>